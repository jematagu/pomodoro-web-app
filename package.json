{
  "name": "tomatoro",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bluebird": "^3.5.2",
    "bootstrap": "^4.1.3",
    "moment": "^2.22.2",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "raven-js": "^3.27.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-icons-kit": "^1.1.7",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
